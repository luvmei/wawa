<%- contentFor('css') %>
<link href="https://cdn.jsdelivr.net/npm/quill@2.0.0-rc.2/dist/quill.snow.css" rel="stylesheet">

<%- contentFor('body') %>
<div class="container-fluid">
  <div class="row">
    <div class="col-lg-6">
      <div class="card">
        <div class="card-header">
          공지사항 및 이벤트
        </div>
        <div class="card-body">
          <table id="notice" class="cell-border hover" style="width: 100%">
            <thead>
              <tr>
                <th>IDX</th>
                <th>카테고리</th>
                <th>작성일자</th>
                <th>종류</th>
                <th>공개범위</th>
                <th>제목</th>
                <th>내용</th>
                <th>노출여부</th>
                <th>팝업</th>
                <th></th>
                <th></th>
              </tr>
            </thead>
          </table>
        </div>
      </div>
    </div>
    <div class="col-lg-6">
      <div class="card">
        <div class="card-header" style="font-size: 1rem">
          메세지
        </div>
        <div class="card-body">
          <table id="message" class="cell-border hover" style="width: 100%">
            <thead>
              <tr>
                <th>IDX</th>
                <th>보낸시간</th>
                <th>종류</th>
                <th>제목</th>
                <th>내용</th>
                <th>받는유저</th>
                <th>내용보기</th>
              </tr>
            </thead>
          </table>
        </div>
      </div>
    </div>
    <div class="col-lg-6">
      <div class="card">
        <div class="card-header" style="font-size: 1rem">
          문의
        </div>
        <div class="card-body">
          <table id="qna" class="cell-border hover" style="width: 100%">
            <thead>
              <tr>
                <th>IDX</th>
                <th>문의시간</th>
                <th>종류</th>
                <th>문의유저</th>
                <th>제목</th>
                <th>내용</th>
                <th>답변</th>
                <th>상태</th>
              </tr>
            </thead>
          </table>
        </div>
      </div>
    </div>
    <div class="col-lg-6 d-none">
      <div class="card">
        <div class="card-header" style="font-size: 1rem">
          이벤트 배너
        </div>
        <div class="card-body">
          <table id="eventBanner" class="cell-border hover" style="width: 100%">
            <thead>
              <tr>
                <th>IDX</th>
                <th>순번</th>
                <th>제목</th>
                <th></th>
                <th></th>
              </tr>
            </thead>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>

<%- contentFor('script') %>
<script src="https://cdn.jsdelivr.net/npm/quill@2.0.0-rc.2/dist/quill.js"></script>
<script src="/assets/script/board.js"></script>