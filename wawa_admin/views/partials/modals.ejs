              class="form-control rate-input"
              id="slot-roll-rate"
              placeholder="슬롯 롤링요율"
              maxlength="5"
              name="slot_roll"
              data-ratetype="슬롯 롤링요율"
              required
            />
          </div>
          <div class="col-12 col-lg-6 my-3">
            <input
              type="text"
              class="form-control rate-input"
              id="lose-rate"
              placeholder="루징요율"
              maxlength="5"
              name="lose_rate"
              data-ratetype="루징요율"
              required
            />
          </div>
          <div class="col-12 col-lg-6 my-3 d-none">
            <input
              type="text"
              class="form-control rate-input"
              id="bet-margin-rate"
              placeholder="베팅마진 요율"
              maxlength="5"
              name="bet-margin_rate"
              data-ratetype="베팅마진요율"
              value="0"
              required
            />
          </div>
          <div class="col-12 col-lg-6 my-3 d-none">
            <input
              type="text"
              class="form-control rate-input"
              id="roll-margin-rate"
              placeholder="롤링마진 요율"
              maxlength="5"
              name="roll-margin_rate"
              data-ratetype="롤링마진요율"
              value="0"
              required
            />
          </div>
          <div class="col-12 fw-bold fs-5 mt-4 d-none" id="reg-domain-info" value>가입코드 정보</div>
          <div class="col-12 col-lg-6 d-none" id="reg-domain-input">
